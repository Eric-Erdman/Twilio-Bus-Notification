#Loop to continuously check users schedules and match them with times

current_time = datetime.now(central)
current_day = current_time.strftime("%A")
current_time = current_time.strftime("%H:%M")

if current_day.lower() == DayM.lower() and current_time == resultM:
    print("It's time for Monday's schedule")
    #SEND STOPM to transitland
    stop_url = stopFinder(locationM)
